	<bean id="storeSusceptibilityMap" class="it.sinergis.ep09.landslide.wps.StoreSusceptibilityMap">
    	<constructor-arg index="0" ref="catalog" />
	</bean>
	
	<bean id="computeFloodProneBaseMap" class="it.sinergis.ep09.flooding.wps.ComputeFloodProneBaseMap">
    	<constructor-arg index="0" ref="catalog" />
	</bean>
	
	<bean id="computeTopographicIndexMap" class="it.sinergis.ep09.flooding.wps.ComputeTopographicIndexMap">
    	<constructor-arg index="0" ref="catalog" />
	</bean>
	
	<bean id="computeFinalFloodProbabilityMap" class="it.sinergis.ep09.flooding.wps.ComputeFinalFloodProbabilityMap">
    	<constructor-arg index="0" ref="catalog" />
	</bean>
	
	<bean id="rasterToVectorGdal" class="it.sinergis.ep09.landslide.wps.RasterToVectorGdal">
    	<constructor-arg index="0" ref="catalog" />
