<?xml version="1.0" encoding="UTF-8"?>
 <!DOCTYPE beans PUBLIC "-//SPRING//DTD BEAN//EN" "http://www.springframework.org/dtd/spring-beans.dtd">
<beans>
	<bean id="createReclassificationTable" class="it.sinergis.ep09.landslide.wps.CreateReclassificationTable">
    	<constructor-arg index="0" ref="catalog" />
	</bean>
	
	<bean id="computeLandslideSusceptibilityMap" class="it.sinergis.ep09.landslide.wps.ComputeLandslideSusceptibilityMap">
    	<constructor-arg index="0" ref="catalog" />
	</bean>
	
	<bean id="landslideValidation" class="it.sinergis.ep09.landslide.wps.LandslideValidation">
    	<constructor-arg index="0" ref="catalog" />
